<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_graph"
    app:startDestination="@id/datasetFragment">

    <fragment
        android:id="@+id/datasetFragment"
        android:name="org.tensorflow.lite.examples.bertqa.fragments.DatasetFragment"
        android:label="DatasetFragment">
        <action
            android:id="@+id/action_datasetFragment_to_qaFragment"
            app:destination="@id/qaFragment" />
    </fragment>
    <fragment
        android:id="@+id/qaFragment"
        android:name="org.tensorflow.lite.examples.bertqa.fragments.QaFragment"
        android:label="QaFragment">
        <argument
            android:name="datasetPosition"
            app:argType="integer" />
    </fragment>
</navigation>
